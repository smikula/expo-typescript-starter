{
    "compilerOptions": {
        "skipLibCheck": true,
        "target": "es5",
        "module": "es2015",
        "jsx": "react",
        "outDir": "./lib",
        "rootDir": "./src",
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
    }
}
